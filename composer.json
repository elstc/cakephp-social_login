{
  "name": "Elastic/SocialLogin",
  "description": "Elastic/SocialLogin plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Elastic\\SocialLogin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elastic\\SocialLogin\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}

{
  "name": "nojimage/cakephp-social_login",
  "description": "SocialLogin plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.6.3",
    "cakephp/cakephp": "^3.4",
    "hybridauth/hybridauth": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Elastic\\SocialLogin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elastic\\SocialLogin\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}

{
  "name": "elstc/cakephp-social_login",
  "description": "SocialLogin plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.6.3",
    "cakephp/cakephp": "^3.6",
    "hybridauth/hybridauth": "^3.7"
  },
  "require-dev": {
      "phpunit/phpunit": "^5.7|^6.5"
  },
  "autoload": {
    "psr-4": {
      "Elastic\\SocialLogin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elastic\\SocialLogin\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
